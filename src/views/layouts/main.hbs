    {{else}}
      {{>header}}
      {{>data-i18n}}
      {{#if (isAgente user.Rol)}} {{else}} {{>nav}} {{/if}}
      {{>messages}}
      {{{body}}}
      {{>footer}}
    {{/if}}
  {{else}}
    {{>messages}}
    {{{body}}}    
  {{/if}}


</body>


